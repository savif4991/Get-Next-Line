#include "get_next_line.h"
#include <stdio.h>
#include <fcntl.h>
int main()
{
	int fd = open("./test", O_RDONLY);
	//close(fd);
	printf("%s\n", get_next_line(fd));
	printf("%s\n", get_next_line(fd));
	printf("%s\n", get_next_line(fd));
	printf("%s\n", get_next_line(fd));
	printf("%s\n", get_next_line(fd));
	return (0);
}